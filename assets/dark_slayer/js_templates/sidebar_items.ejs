<script type="text/javascript">
$(document).ready(function () {
	$('img[id^="lattest_"]').each(function () {
		App.initializeTooltip($(this), true, 'warehouse/item_info_image');
	});
});	
</script>
<% 
if(typeof items != 'undefined'){ 
if(items != false){
%>
<div class="itemsBlock flex-c-c">
<% 
for(var i=0; i< items.length; i++) { 
%>
<div class="item">
	<div class="item-img flex-c-c">
		<img id="lattest_<%= i+1 %>" data-info="<%= items[i].item %>" src="<%= items[i].image.replace("interface/", "") %>" alt="">
	</div>
	<div class="item-name">
		<%= items[i].name %>
	</div>
	<div class="item-price">
		<%= items[i].price %>
	</div>
	<div class="item-buy">
		<a href="<%= base_url %>market/buy/<%= items[i].id %>">Buy Item</a>
	</div>
</div>
<% } %>
</div>
<%
}
else{
%>
<div class="alert alert-warning" role="alert" style="margin-top:10px;"><%= App.lc.translate('No Items Found.').fetch() %></div>
<%
}
} 
%>