<script type="text/javascript">
$(document).ready(function () {
	$('div[id^="lattestimg_"]').each(function () {
		App.initializeTooltip($(this), true, 'warehouse/item_info_image');
	});
});	
</script>
<% 
if(typeof items != 'undefined'){ 
if(items != false){
%>
<%
for(var i=0; i< items.length; i++) { 
%>
<div class="marketContent flex-s-c" id="lattestimg_<%= i+1 %>" data-info='<%= items[i].item %>'>
	<div class="marketContent_img">
		<img src="<%= items[i].image %>" alt="">
	</div>
	<div class="marketContent_info">
		<span class="marketContent_info-link"><%= items[i].name %></span>
		<div class="marketContent_info-text">
			<%= App.lc.translate('Price').fetch() %>: <%= items[i].price %>
		</div>
	</div>
	<a href="<%= base_url %>market/buy/<%= items[i].id %>" class="marketContent_number"><%= App.lc.translate('buy').fetch() %></a>
</div>
<% } %>
<%
}
else{
%>
	<div class="alert alert-info"><%= App.lc.translate('No Items Found.').fetch() %></div>
<%
}
} 
%>

