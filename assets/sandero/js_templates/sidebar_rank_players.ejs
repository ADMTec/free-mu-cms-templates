<% 
if(typeof players != 'undefined'){ 
if(players != false){
%>
<ul class="top-block">
	<li class="top-title">
		<span class="top-number">#</span> <% if(config.player.display_country == 1) { %><span class="top-flag"></span><% } %> <span class="top-name"><%= App.lc.translate('Name').fetch() %></span> <span class="top-lvl"><%= App.lc.translate('LvL').fetch() %><% if(config.player.display_master_level == 1) { %><sup><%= App.lc.translate('ML').fetch() %></sup><% } %></span><% if(config.player.display_resets == 1) { %> <span class="top-Res"><%= App.lc.translate('Res').fetch() %><% if(config.player.display_gresets == 1) { %><sup><%= App.lc.translate('GR').fetch() %></sup><% } %></span><% } %>
	</li>
	<% for(var i=0; i<players.length; i++) { %>
	<li class="top-title">
		<span class="top-number"><%= i+1 %></span> <% if(config.player.display_country == 1) { %><span class="top-flag"><span class="f16"><span class="flag <%= players[i].country %>"></span></span></span><% } %> <span class="top-name"><a href="<%= base_url %>character/<%= players[i].name_hex %>/<%= server_selected %>"><%= players[i].name %></a></span> <span class="top-lvl"><%= players[i].level %><% if(config.player.display_master_level == 1) { %><sup><%= players[i].mlevel %></sup><% } %></span><% if(config.player.display_resets == 1) { %> <span class="top-Res"><%= players[i].resets %><% if(config.player.display_gresets == 1) { %><sup><%= players[i].gresets %></sup><% } %></span><% } %>
	</li>
	<% } %>
</ul>
<% 
}
else{
%>
<div class="alert alert-info"><%= App.lc.translate('No Players Found').fetch() %></div>
<%
}
} 
%>