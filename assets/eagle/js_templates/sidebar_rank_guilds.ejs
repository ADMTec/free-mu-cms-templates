<% 
if(typeof guilds != 'undefined'){ 
if(guilds != false){
%>
<div class="table">
	<% for(var i=0; i<guilds.length; i++) { %>
	<div class="table-row <% if((i+1) < 4){ %>table-row-top<% } else { %>table-row<% } %>">
		<div class="table-td number"><span<% if((i+1) < 4){ %> class="n-top-<%= i+1 %>"<% } %>><%= i+1 %></span></div>
		<div class="table-td name"><a href="<%= base_url %>guild/<%= guilds[i].name_hex %>/<%= server_selected %>"><%= guilds[i].name %></a></div>
		<div class="table-td points"><img src="<%= base_url %>rankings/get_mark/<%= guilds[i].mark %>/16" style="border: 0;" /></div>
		<div class="table-td points"><%= guilds[i].score %></div>
	</div>
	<% } %>
</div>
<% 
}
else{
%>
<div class="alert alert-info"><%= App.lc.translate('No Guilds Found').fetch() %></div>
<%
}
} 
%>